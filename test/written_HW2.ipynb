   "outputs": [],
   "source": [
    "def generate_test_set(N: int) -> Tuple[np.ndarray, np.ndarray]:\n",
    "    X = (np.random.rand(N,2) - 0.5)*2\n",
    "    y = np.sign(0.5*X[:,0]+0.5*X[:,1])\n",
    "    return X, y"
   ]
